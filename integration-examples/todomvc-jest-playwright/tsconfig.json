{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": ["ESNext"],
    "isolatedModules": true,
    "strict": true,
    "types": ["node", "jest", "jest-playwright-preset", "expect-playwright"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true
  }
}
